{
  "name": "battle-log",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "build-gh": "run-p type-check \"build-only-gh {@}\" --",
    "build-only-gh": "vite build --base=/TornRankedWarReport/",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "vue": "^3.5.20",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "~10.4.0",
    "gh-pages": "^6.3.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "sass-embedded": "^1.91.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  }
}